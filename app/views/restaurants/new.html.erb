<div class="container mb-3">
  <h2 class="mt-3">Add a new restaurant to the list</h2>
  <%= simple_form_for @restaurant do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :category %>
    <%= f.input :phone_number %>
    <%= f.submit %>
  <% end %>
  <div class="container mt-4" style="padding-left: 0">
    <%= link_to 'Back to list', restaurants_path %>
  </div>
</div>
